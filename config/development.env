# Local development defaults for the core API
APP_ENV=dev
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=INFO

# Use on-disk SQLite for easy local setup
DATABASE_URL=sqlite:///./data/dev_platform.db
VECTOR_DB_PATH=./data/vector_store

# API configuration
API_PORT=8002
REALTIME_API_PORT=8001
API_V1_PREFIX=/api
CORS_ORIGINS=http://localhost:3000,http://localhost:5173,http://127.0.0.1:3000

# Feature toggles
ENABLE_REAL_TIME=true
ENABLE_GITHUB_INTEGRATION=true
ENABLE_ANALYTICS=true
ENABLE_AI_ASSISTANCE=true

# AI & Feedback Rate Limiting (PR-32a)
RL_AI_GEN_PM=10
RL_AI_GEN_PH=100
RL_AI_GEN_BURST=3
RL_AI_APPLY_PM=6
RL_AI_APPLY_PH=60
RL_AI_APPLY_BURST=2
RL_FB_PM=60
RL_FB_PH=1200
RL_FB_BURST=20
RL_FB_STATS_PM=30
RL_FB_STATS_PH=600
RL_FB_STATS_BURST=10
