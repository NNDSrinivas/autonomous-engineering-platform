# Local development defaults for the core API
APP_ENV=dev
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=INFO

# Use PostgreSQL from Docker container
DATABASE_URL=postgresql://mentor:mentor@localhost:5432/mentor
VECTOR_DB_PATH=./data/vector_store

# Redis configuration for caching and rate limiting
REDIS_URL=redis://localhost:6379

# API configuration
API_PORT=8002
REALTIME_API_PORT=8001
API_V1_PREFIX=/api
# Explicit origins for dev; add/remove as needed. Use comma-separated, not JSON.
CORS_ORIGINS=http://localhost:3000,http://localhost:3007,http://localhost:3008,http://localhost:5173,http://127.0.0.1:3000,http://127.0.0.1:3007,http://127.0.0.1:3008,http://localhost:8000,https://auth.navralabs.com,https://aep.navralabs.com

# Auth0 Configuration
AUTH0_DOMAIN=dev-nr76km00xa820k15.us.auth0.com
AUTH0_CLIENT_ID=6aJ5nY80nZmZkVTiv6PxLIZ8EiFEdqT3
AUTH0_AUDIENCE=https://api.navralabs.com
AUTH0_ALGORITHM=RS256

# AEP JWT Session Management
# Provide this via a local, untracked env file or shell export to avoid committing secrets.
# Example: export AEP_JWT_SECRET=$(openssl rand -base64 48)
AEP_JWT_SECRET=
AEP_JWT_ISSUER=https://api.navralabs.com
AEP_JWT_TTL_SECONDS=3600
AEP_JWT_ALG=HS256

# OpenAI API Configuration
# Provide this via a local, untracked env file or shell export to avoid committing secrets.
# Example: export OPENAI_API_KEY=sk-...
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini

# Feature toggles
ENABLE_REAL_TIME=true
ENABLE_GITHUB_INTEGRATION=true
ENABLE_ANALYTICS=true
ENABLE_AI_ASSISTANCE=true

# AI & Feedback Rate Limiting (PR-32a)
RL_AI_GEN_PM=10
RL_AI_GEN_PH=100
RL_AI_GEN_BURST=3
RL_AI_APPLY_PM=6
RL_AI_APPLY_PH=60
RL_AI_APPLY_BURST=2
RL_FB_PM=60
RL_FB_PH=1200
RL_FB_BURST=20
RL_FB_STATS_PM=30
RL_FB_STATS_PH=600
RL_FB_STATS_BURST=10
