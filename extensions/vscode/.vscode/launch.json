{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run AEP Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}/extensions/vscode"],
      "outFiles": ["${workspaceFolder}/extensions/vscode/out/**/*.js"],
      "env": { "AEP_CORE_API": "http://localhost:8002", "AEP_REALTIME_API": "http://localhost:8001" }
    }
  ]
}